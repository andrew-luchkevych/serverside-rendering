	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "ts-jest"
		},
		"testMatch": [
			"**/tests/*.spec.(ts|tsx|js)"
		],
		"setupFiles": [
			"<rootDir>config/jest/bootstrap.js"
		],
		"collectCoverageFrom": [
			"src/client/**/*.ts",
			"src/client/**/*.tsx",
			"!src/client/index.tsx",
			"!src/client/routes.tsx",
			"!**/node_modules/**",
			"!**/dist/**"
		],
		"collectCoverage": true,
		"bail": true,
		"moduleNameMapper": {
			"^.+\\.s?css$": "identity-obj-proxy"
		},
		"coverageDirectory": "./coverage/"
	}
}
